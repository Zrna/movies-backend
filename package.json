{
  "author": "Luka Zrnic",
  "name": "reviews-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon index.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "migration:generate": "npx sequelize migration:generate --name",
    "migration:run": "npx sequelize db:migrate",
    "migration:undo": "npx sequelize db:migrate:undo",
    "migrations:rerun": "npx sequelize db:migration"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix"
    ],
    "*.+(json|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "swagger-jsdoc": "^6.0.2",
    "swagger-ui-express": "^4.1.6"
  }
}
